<%- include('../partials/header')%>
<%- include('../partials/bootstrapHeader') %>

<main>
    <div class="container py-4">
      <!--search input-->
        <form action="/searchEnroll" class="form-control d-flex search mb-4" method="post">
            <input class="form-control" required name="enroll" placeholder="Type to search...">
            <button class="btn btn-outline-secondary" name="searchEnrollment" type="submit">Search</button>
        </form> 
        <%foundEnrollment.forEach((student) =>{%>
          <form action="/removeIssuedBook" method="post">
            <div class="h-100 p-1 mb-2 bg-light border rounded-3">
              <div class="container-fluid py-0">
                <a class="enrollNo" href="/dashboard/<%=student.enrollment%>"><h4 class="display-7 fw-bold text-success"><%=student.enrollment%></h4></a>
              </div>
            </div>
          </form>
      <%})%>
    </div>
  </main>
<%- include('../partials/footer')%>